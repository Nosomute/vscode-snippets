		"prefix": [
			"echo",
			"eco",
			"ec"
		],
		"body": [
			"echo ${2:\"${0:}\"} . \"\\n\";",
		],
	},
	". \"\\n\"": {
		"prefix": [
			". \"\\n\"",
		],
		"body": [
			". \"\\n\""
		],
	},
